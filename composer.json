{
    "name": "aqua/aquastrap",
    "description": "use laravel blade component methods as javascript function for XHR",
    "keywords": [
        "Laravel",
        "Blade Component"
    ],
    "homepage": "https://github.com/aquaa/aquastrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Sourav Rakshit",
            "email": "aqua@itsrav.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/contracts": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "illuminate/view": "^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Aqua\\Aquastrap\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aqua\\Aquastrap\\AquastrapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
